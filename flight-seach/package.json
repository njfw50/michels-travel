{
  "name": "michels-flight-search",
  "version": "2.0.0",
  "description": "Sistema completo de busca de voos com APIs gratuitas, IA integrada e experiência extraordinária",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'Build completed'",
    "test": "jest",
    "setup": "node setup.js",
    "init-db": "node -e \"require('./src/services/databaseService.js').init()\"",
    "backup": "bash scripts/backup.sh",
    "docker-build": "docker build -t michels-flight-search .",
    "docker-run": "docker run -p 3000:3000 michels-flight-search",
    "docker-compose": "docker-compose up -d",
    "docker-stop": "docker-compose down",
    "logs": "tail -f logs/app.log",
    "health": "curl -f http://localhost:3000/api/health || echo 'Server not running'"
  },
  "keywords": [
    "flight",
    "search",
    "booking",
    "travel",
    "api",
    "ai",
    "pagination"
  ],
  "author": "Michel's Travel",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "axios": "^1.6.0",
    "node-cache": "^5.1.2",
    "sqlite3": "^5.1.6",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
